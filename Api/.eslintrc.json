{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures":{
      "experimentalObjectRestSpread": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true,
      "defaultParams": true,
      "destructuring": true
    }
  },
  "rules": {

  }
}
